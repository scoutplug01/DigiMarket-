{
  "version": 2,
  "routes": [
    {
      "src": "/(.*\\.(css|js|jpg|jpeg|png|gif|svg|ico|woff|woff2|ttf|eot|json))",
      "dest": "/project-root/$1"
    },
    {
      "src": "/",
      "dest": "/project-root/index.html"
    },
    {
      "src": "/index.html",
      "dest": "/project-root/index.html"
    },
    {
      "src": "/home.html",
      "dest": "/project-root/home.html"
    },
    {
      "src": "/market.html",
      "dest": "/project-root/market.html"
    },
    {
      "src": "/cart.html",
      "dest": "/project-root/cart.html"
    },
    {
      "src": "/checkout.html",
      "dest": "/project-root/checkout.html"
    },
    {
      "src": "/confirm-payment.html",
      "dest": "/project-root/confirm-payment.html"
    },
    {
      "src": "/payment-success.html",
      "dest": "/project-root/payment-success.html"
    },
    {
      "src": "/invoice.html",
      "dest": "/project-root/invoice.html"
    },
    {
      "src": "/product-details.html",
      "dest": "/project-root/product-details.html"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/project-root/auth/$1"
    },
    {
      "src": "/user/(.*)",
      "dest": "/project-root/user/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/project-root/admin/$1"
    }
  ]
}